e title \r\n        document.title= 'Sign Up - Notedly';\r\n    \r\n    });\r\n\r\n    return (\r\n     <React.Fragment>\r\n         <UserForm action={signUp} formType=\"signup\"/>\r\n         {loading && <p>Loading...</p>}\r\n         {error && <p>Error creating an account!</p>}\r\n     </React.Fragment>\r\n            \r\n           \r\n        \r\n    );\r\n};\r\n\r\nexport default SignUp;\r\n"},"lineCount":null}},"error":null,"hash":"27ea6b72d1f69306d52d648413f6df2d","cacheData":{"env":{}}}