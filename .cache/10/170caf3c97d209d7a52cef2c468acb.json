gment>\r\n        )}\r\n        <label htmlFor=\"email\">Email:</label>\r\n        <input\r\n        required\r\n        type=\"email\"\r\n        id=\"email\"\r\n        name=\"email\"\r\n        placeholder=\"Email\"\r\n        onChange={onChange}\r\n        />\r\n        <label htmlFor=\"password\">Password:</label>\r\n        <input\r\n        required\r\n        type=\"password\"\r\n        id=\"password\"\r\n        name=\"password\"\r\n        placeholder=\"Password\"\r\n        onChange={onChange}\r\n        />\r\n        <Button type=\"submit\">Submit</Button>\r\n        </Form>\r\n        </Wrapper>\r\n        );\r\n        };\r\n\r\nexport default UserForm;\r\n"},"lineCount":null}},"error":null,"hash":"de8ec3bfe884e0fff5b68377b2380a4f","cacheData":{"env":{}}}