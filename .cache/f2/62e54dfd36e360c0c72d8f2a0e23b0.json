ur(5px);\r\n    box-shadow: 20px 20px 40px -6px rgba(0,0,0,0.2);\r\n    text-align:center;\r\n    position:relative;\r\n    -webkit-transform: all 0.2s ease-in-out;\r\n    \r\n    h2{\r\n        font-weight:500;\r\n        color: #fff;\r\n        opacity: 0.7;\r\n        font-size: 1.4rem;\r\n        margin-top: 0;\r\n        margin-bottom: 60px;\r\n        text-shadow: 2px 2px 4px rgba(0,0,0,0.2)\r\n    }\r\n`;\r\n\r\nconst Metadata= styled.div`\r\n@media(min-width: 500px){\r\n    display: flex;\r\n    align-items:top;\r\n}\r\n\r\n`;\r\n\r\nconst MetaInfo= styled.div`\r\n    padding-right:1em;\r\n`;\r\n\r\nconst UserActions= styled.div`\r\n    margin-left:auto;\r\n`;\r\n\r\n\r\n\r\nconst Note= ({note}) =>{\r\n    return(\r\n        <StyledNote>\r\n            <Metadata>\r\n            <MetaInfo>\r\n            <img \r\n                src={note.author.avatar}\r\n                alt=\"{note.author.username} avatar\"\r\n                height=\"50px\"\r\n            />\r\n            </MetaInfo>\r\n            <MetaInfo>\r\n                <em>By</em> {note.author.username} <br/>\r\n                {format(note.createdAt, 'MMM Do YYYY')}\r\n            </MetaInfo>\r\n            <UserActions>\r\n                <em>Favorites:</em> {note.favoriteCount}\r\n            </UserActions>\r\n            </Metadata>                    \r\n            <ReactMarkdown source={note.content}/>\r\n        </StyledNote>\r\n    );\r\n};\r\n\r\nexport default Note;"},"lineCount":null}},"error":null,"hash":"8dd6f4c6f62d6791dca5e884299194ba","cacheData":{"env":{}}}